        "build": "yarn install"
    },
    "author": "Gaurav Walia",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.5.1",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.1",
        "mongoose": "^7.6.2",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "dotenv": "^16.3.1",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.1",
        "mocha": "^10.2.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3",
        "supertest": "^6.3.3"
    },
    "lint-staged": {
        "*.js": "prettier --write"
    },
    "husky": {
        "hooks": {
