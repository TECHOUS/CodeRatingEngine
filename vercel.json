{
    "version": 2,
    "github": {
        "enabled": false
    },
    "builds":[
        {
            "src": "./bin/www",
            "use": "@vercel/node",
            "config": { "includeFiles": ["**"] }
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "bin/www"
        }
    ]
}